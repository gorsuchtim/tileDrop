olor = \"lit--white\")\r\n    : (globals.game.flashColor = \"lit--green\");\r\n\r\n  const tile = setTile();\r\n  util.elementLib.classChange(tile, \"add\", globals.game.flashColor);\r\n  tile.addEventListener(\"click\", function () {\r\n    scoring(this);\r\n  });\r\n  //util.setEvents.methods.dynamicEvents();\r\n\r\n  increaseCount();\r\n\r\n  setTimeout(() => {\r\n    const tile = document.querySelector(\".tile\");\r\n    // comment this out so we can debug Scoring.js tile.firstElementChild.nextElementSibling\r\n    // tile.parentNode.removeChild(tile);\r\n  }, 850); // set to music.bpm\r\n};\r\n\r\nexport default flashTile;\r\n"},"lineCount":null}},"error":null,"hash":"b7ae5e0c93ddc53eff5c5baa67115259","cacheData":{"env":{}}}